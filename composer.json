{
    "name": "doctrine/doctrine-static-website-generator-bundle",
    "description": "Symfony DoctrineSkeletonMapperBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan H. Wage",
            "email": "jonwage@gmail.com",
            "homepage": "https://jwage.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-skeleton-mapper-bundle": "dev-master",
        "doctrine/rst-parser": "dev-master",
        "doctrine/skeleton-mapper": "dev-master",
        "doctrine/static-website-generator": "dev-master",
        "erusev/parsedown": "^1.7",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^5.0",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-deprecation-rules": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpstan/phpstan-strict-rules": "^0.10",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\Bundle\\DoctrineStaticWebsiteGeneratorBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\Bundle\\DoctrineStaticWebsiteGeneratorBundle\\Tests\\": "tests/"
        }
    }
}
